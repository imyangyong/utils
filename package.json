{
  "name": "@imyangyong/utils",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@9.0.6",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @antfu",
  "author": "Angus Yang <imyangyong@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/imyangyong/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imyangyong/utils.git"
  },
  "bugs": {
    "url": "https://github.com/imyangyong/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --no-tag && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@imyangyong/eslint-config": "^1.7.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.12.12",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "p-limit": "^5.0.0",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "throttle-debounce": "5.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
